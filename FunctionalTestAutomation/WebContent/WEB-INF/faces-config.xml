<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<managed-bean>
		<managed-bean-name>controller</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.controller.Controller</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.bean.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>serviceBean</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.bean.ServiceBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataBean</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.bean.DataBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uIBean</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.bean.UIBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>testCaseBean</managed-bean-name>
		<managed-bean-class>com.tracfone.testcaseapp.bean.TestCaseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/jsp/Login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/jsp/ServiceTestCase.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/ServiceTestCase.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/jsp/AddTestCase.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/AddTestCase.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/jsp/ServiceTestCase.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
